#h1 Backlog
- if @user_stories
  %table
    %tr
      %th Role
      %th Action
      %th Result
      %th Priority
      %th Estimated Points
    - @user_stories.each do |user_story|
      %tr
        %td= user_story.role
        %td= user_story.action
        %td= user_story.result
        %td= user_story.priority
        %td= user_story.estimated_points
= link_to 'New User Story', new_user_story_path
